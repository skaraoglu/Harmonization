Cell[BoxData[
 RowBox[{
  RowBox[{"getoutofscalenotes", "[", 
   RowBox[{"notes_", ",", " ", "scale_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", "=", "notes"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"outs", " ", "=", " ", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", 
        RowBox[{"Length", "[", "n", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"MemberQ", "[", 
            RowBox[{
             RowBox[{"scalenotes", "[", 
              RowBox[{"rootnote", ",", "scale"}], "]"}], ",", 
             RowBox[{"n", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}], "]"}], "==", "False"}], "&&", 
          RowBox[{
           RowBox[{"n", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ">", "0"}]}], " ", ",", 
         "\[IndentingNewLine]", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"outs", ",", " ", 
           RowBox[{"n", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], "]"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "outs", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
